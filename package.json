{
  "name": "quadstore",
  "version": "7.0.1-alpha.1",
  "description": "A LevelDB-backed graph database for Node.js and the browser. Supports quads, RDF/JS interfaces and SPARQL queries.",
  "keywords": [
    "node",
    "nodejs",
    "quadstore",
    "graph",
    "rdf",
    "database",
    "sparql",
    "rdfjs",
    "browser"
  ],
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "scripts": {
    "test": "nyc mocha ./test",
    "test-rdf": "rdf-test-suite -c .rdf-test-suite-cache/ test-rdf/engine.js https://w3c.github.io/rdf-tests/sparql11/data-sparql11/manifest-all.ttl -s http://www.w3.org/TR/sparql11-query/ -t http://www.w3.org/2009/sparql/docs/tests/data-sparql11/aggregates/",
    "ts:build:cjs": "tsc --project ./tsconfig.json && echo \"{ \\\"type\\\": \\\"commonjs\\\" }\" > dist/cjs/package.json",
    "ts:watch:cjs": "mkdir -p dist/cjs && echo \"{ \\\"type\\\": \\\"commonjs\\\" }\" > dist/cjs/package.json && tsc --watch --project ./tsconfig.json",
    "ts:build:esm": "tsc --project ./tsconfig-esm.json && echo \"{ \\\"type\\\": \\\"module\\\" }\" > dist/esm/package.json",
    "ts:watch:esm": "mkdir -p dist/esm && echo \"{ \\\"type\\\": \\\"module\\\" }\" > dist/esm/package.json && tsc --watch --project ./tsconfig-esm.json",
    "ts:build": "npm run ts:build:cjs && npm run ts:build:esm"
  },
  "files": [
    "dist/esm/**/*.js",
    "dist/esm/**/*.d.ts",
    "dist/cjs/**/*.js",
    "dist/cjs/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/beautifulinteractions/node-quadstore.git"
  },
  "devDependencies": {
    "@rdfjs/data-model": "^1.1.2",
    "@types/abstract-leveldown": "^5.0.1",
    "@types/levelup": "^4.3.0",
    "@types/sparqljs": "^3.0.1",
    "abstract-leveldown": "^6.3.0",
    "benchmark": "^2.1.4",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "leveldown": "^5.6.0",
    "memdown": "^5.1.0",
    "mocha": "^8.1.0",
    "n3": "^1.6.1",
    "nyc": "^15.1.0",
    "rdf-test-suite": "^1.13.1",
    "rdflib": "^1.3.1",
    "should": "^13.2.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "asynciterator": "^3.0.0",
    "js-sorted-set": "^0.6.0",
    "levelup": "^4.4.0",
    "p-reduce": "^2.1.0",
    "sparqljs": "^3.0.3"
  },
  "author": "Jacopo Scazzosi <me@jacoscaz.com>",
  "contributors": [
    "Matteo Murgida <teomurgi@gmail.com>"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/beautifulinteractions/node-quadstore",
  "bugs": "https://github.com/beautifulinteractions/node-quadstore/issues",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
